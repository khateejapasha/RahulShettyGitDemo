package Ecommerce;

import java.time.Duration;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

import io.github.bonigarcia.wdm.WebDriverManager;

public class StandaloneTest {

	public static void main(String[] args) {
		WebDriverManager.chromedriver().setup();//used to set the chromedriver systemproperty
		WebDriver driver=new ChromeDriver();
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(10));
		driver.get("https://rahulshettyacademy.com/client/#/auth/login");
		driver.findElement(By.id("userEmail")).sendKeys("Khateejapasha11@gmail.com");
		driver.findElement(By.id("userPassword")).sendKeys("ZoyaMajid@100");
		driver.findElement(By.id("login")).click();
		
	}
	

}
