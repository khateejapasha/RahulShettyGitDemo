package AmazonTest;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.PageFactory;
import org.testng.annotations.Test;

import Amazon.Amazonhome;

public class AmazonTestClass extends AmazonBase{
	
	@Test
	public void InitializeBrowser() throws InterruptedException
	{
		Amazonhome pf1 = PageFactory.initElements(driver,Amazonhome.class);
		pf1.Source();
		Thread.sleep(2000);
		pf1.Destination();
		pf1.Calendar();
	}
	

}
