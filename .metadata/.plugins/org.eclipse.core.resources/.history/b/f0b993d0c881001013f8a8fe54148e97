package AmazonTest;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.PageFactory;
import org.testng.annotations.Test;

import Amazon.Amazonhome;

public class AmazonTestClass extends AmazonBase{
	WebDriver driver;
	@Test
	public void InitializeBrowser() throws InterruptedException
	{
		launchBrowser();
		
		Amazonhome pf1 = PageFactory.initElements(driver,Amazonhome.class);
		pf1.Search();
		Thread.sleep(2000);
		pf1.Searchmagnifier();
	}
	

}
